{
  "name": "storybook-addon-responsiveness",
  "version": "1.0.0",
  "description": "Storybook addon to preview components using various viewport sizes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/strothj/storybook-addon-responsiveness.git",
  "author": "strothj <https://github.com/strothj>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">= 8.9",
    "yarn": ">= 1.2.1"
  },
  "scripts": {
    "clean": "rimraf dist storybook",
    "build": "npm-run-all clean build:tsc",
    "build:tsc": "tsc -p ./tsconfig.build.json",
    "device-list:update": "ts-node tools/update-device-list.ts",
    "lint": "tslint -p ./",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o storybook",
    "test": "jest --config jest.config.js"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "devDependencies": {
    "@storybook/addons": "^3.3",
    "@storybook/react": "^3.3.8",
    "@types/jest": "^22.0.1",
    "@types/lz-string": "^1.3.32",
    "@types/node-fetch": "^1.6.7",
    "@types/react": "^16.0.34",
    "@types/react-test-renderer": "^16.0.0",
    "@types/storybook__react": "^3.0.6",
    "babel-core": "^6.26.0",
    "jest": "^22.0.6",
    "node-fetch": "^1.7.3",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.10.2",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-test-renderer": "16",
    "request": "2.34",
    "request-promise-native": "^1.0.5",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.0.1",
    "ts-loader": "^3.2.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.4.2",
    "tslint-config-prettier": "^1.6.0",
    "tslint-language-service": "^0.9.8",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.6.2"
  },
  "peerDependencies": {
    "@storybook/addons": ">=3.3",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "lz-string": "^1.4.4",
    "typestyle": "^1.6.0"
  }
}
